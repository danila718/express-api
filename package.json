{
  "name": "dashboard-api",
  "version": "1.0.0",
  "description": "API для учебного проекта",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "npm run build && node ./dist/main.js",
    "dev": "nodemon",
    "dev:inspect": "nodemon -e ts,json --exec node --loader ts-node/esm --inspect=localhost:9222 src/main.ts",
    "build": "tsc",
    "generate": "npx prisma generate",
    "lint": "eslint ./src/**",
    "lint:fix": "eslint ./src/** --fix",
    "test": "npx jest",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules npx jest --config jest.e2e.config.ts --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danila718/express-api.git"
  },
  "author": "Daniil Simonyan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danila718/express-api/issues"
  },
  "homepage": "https://github.com/danila718/express-api#readme",
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "tslog": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "prisma": "^4.2.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
